{
  "videos": [
    {
      "input": [
        {
          "i": "assets/jBpLSRm.gif"
        },
        {
          "f": "lavfi",
          "i": "color=color=0x00000000:size=720x1280:duration=2.88,format=rgba,setsar=1:1"
        }
      ],
      "output": "resources/discord.mp4",
      "filter_complex": [
        {
          "istream": "[0:v]",
          "func": { "split": [ "3" ] },
          "ostream": "[v0][v1][v2]"
        },
        {
          "istream": "[v0]",
          "func": { "setpts": "PTS-STARTPTS" },
          "ostream": "[vp0]"
        },
        {
          "istream": "[v1]",
          "func": { "setpts": "(PTS-STARTPTS)" },
          "ostream": "[vp1]"
        },
        {
          "istream": "[v2]",
          "func": { "setpts": "(PTS-STARTPTS)" },
          "ostream": "[vp2]"
        },
        {
          "istream": "[vp0][vp1][vp2]",
          "func": { "concat": "n=3" },
          "ostream": "[video]"
        }
      ],
      "attributes": [
        "no-audio",
        "not-a-build"
      ]
    }
  ]
}
