{
  "videos": [
    {
      "input": [
        { "i": "resources/class-inheritance.mp4" },
        { "i": "resources/classes.mp4" }
      ],
      "output": "build/lead.mp4",
      "attributes": [ "vsync" ],
      "filter_complex": [

        "[0:v]fps=29,trim=start=0:end=3,setpts=PTS-STARTPTS[vstart]",
        "[0:a]atrim=start=0:end=3,asetpts=PTS-STARTPTS[astart]",

        "[0:v]fps=29,trim=start=3:end=9.2,setpts=PTS-STARTPTS[vbackDrop]",
        "[0:a]atrim=start=3:end=9.2,asetpts=PTS-STARTPTS[abackDrop]",

        "[1:v]fps=29,trim=start=3:end=9.2,setpts=PTS-STARTPTS,scale=608x1080,setsar=1:1,fade=in:st=0:d=10:alpha=1[vref]",
        "[1:a]atrim=start=3:end=9.2,asetpts=PTS-STARTPTS,afade=in:st=0:d=10[aref]",

        "[vbackDrop][vref]overlay=(W-w)/2:(H-h)/2,fps=29[vlead]",
        "[abackDrop][aref]amix[alead]",

        "[vstart][vlead]concat=n=2[video]",
        "[astart][alead]concat=v=0:a=1:n=2[audio]"
      ]
    },
    {
      "require": "build/lead.mp4",
      "input": [
        { "i": "build/lead.mp4" },
        { "i": "resources/class-inheritance.mp4" }
      ],
      "output": "build/Class-Inheritance.mp4",
      "attributes": [ "vsync" ],
      "filter_complex": [

        "[1:v]fps=29,trim=start=9.2:end=235.44,setpts=PTS-STARTPTS[vrest]",
        "[1:a]atrim=start=9.2:end=235.44,asetpts=PTS-STARTPTS[arest]",

        "[0:v][vrest]concat=n=2[video]",
        "[0:a][arest]concat=v=0:a=1:n=2[audio]"
      ]
    }
  ]
}

